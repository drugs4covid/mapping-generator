<#tripsreducido> a rr:TriplesMap;
         rml:logicalSource [
                 rml:source "tripsreducido.csv" ;
                 rml:referenceFormulation ql:CSV
         ];
         rr:subjectMap [
                 rr:template "http://vocab.gtfs.org/terms#Incidencia/{}";
                 rr:termType rr:IRI;
                 rr:class Incidencia;
         ];
         rr:predicateObjectMap [
                 rr:predicateMap [
                         a rr:PredicateMap;
                         rr:constant tmjourney:definedBy;
                 ];
                 rr:objectMap [ rr:column "line_id"; rr:datatype xsd:integer];
         ];
         rr:predicateObjectMap [
                 rr:predicateMap [
                         a rr:PredicateMap;
                         rr:constant estraf:numSentidos;
                 ];
                 rr:objectMap [ rr:column "route_id"; rr:datatype xsd:string];
         ];
         rr:predicateObjectMap [
                 rr:predicateMap [
                         a rr:PredicateMap;
                         rr:constant tmorg:operating;
                 ];
                 rr:objectMap [ rr:column "trip_headsign"; rr:datatype xsd:string];
         ];
         rr:predicateObjectMap [
                 rr:predicateMap [
                         a rr:PredicateMap;
                         rr:constant tmcommons:shortName;
                 ];
                 rr:objectMap [ rr:column "trip_short_name"; rr:datatype xsd:string];
         ];
         rr:predicateObjectMap [
                 rr:predicateMap [
                         a rr:PredicateMap;
                         rr:constant dc:description;
                 ];
                 rr:objectMap [ rr:column "direction_id"; rr:datatype xsd:integer];
         ].
<#stopsnuestro> a rr:TriplesMap;
