<#AGENCY>
         a rr:TriplesMap;
         rml:logicalSource [
                 rml:source: "<#DB_source>";
                 rr:sqlVersion "SQL2008";
                 rr:tableName: "AGENCY";
         ];
         rr:subjectMap [
                 rr:template: "http://vocab.gtfs.org/terms#Agency/{agency_id};
                 rr:termType: "rr:IRI";
                 rr:class: "gtfs:Agency";
         ];
         rr:predicateObjectMap [
                 rr:predicate: "http://vocab.gtfs.org/terms#Agency/agency";
                 rr:objectMap: [ rr:column "agency_name"; rr:datatype xsd:string; ];
         ];
         rr:predicateObjectMap [
                 rr:predicate: "http://vocab.gtfs.org/terms#Agency/fareUrl";
                 rr:objectMap: [ rr:column "agency_url"; rr:datatype xsd:string; ];
         ].
