@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix : <http://vocab.linkeddata.es/def/conceptual-mapping#> .
@prefix brick: <https://brickschema.org/schema/Brick#> .
@prefix csvw: <http://www.w3.org/ns/csvw#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dcam: <http://purl.org/dc/dcam/> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dcmitype: <http://purl.org/dc/dcmitype/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix org: <http://www.w3.org/ns/org#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prof: <http://www.w3.org/ns/dx/prof/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix qb: <http://purl.org/linked-data/cube#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix skos1: <http://www.w3.org/2008/05/skos#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix ssn: <http://www.w3.org/ns/ssn/> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix void: <http://rdfs.org/ns/void#> .
@prefix wotsec: <https://www.w3.org/2019/wot/security#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@base <http://vocab.linkeddata.es/def/conceptual-mapping#> .
<#cities> a rr:TriplesMap;
         rml:logicalSource [
                 rml:source "cities.csv" ;
                 rml:referenceFormulation ql:CSV
         ];
         rr:subjectMap [
                 rr:template "http://vocab.linkeddata.es/def/conceptual-mapping#AltMap/{}";
                 rr:termType rr:IRI;
                 rr:class AltMap;
         ];
         rr:predicateObjectMap [
                 rr:predicateMap [
                         a rr:PredicateMap;
                         rr:constant conceptual-mapping:hasSecurityScheme;
                 ];
                 rr:objectMap [ rr:column "city"; rr:datatype xsd:string];
         ];
         rr:predicateObjectMap [
                 rr:predicateMap [
                         a rr:PredicateMap;
                         rr:constant dcat:endpointURL;
                 ];
                 rr:objectMap [ rr:column "	population"; rr:datatype xsd:integer];
         ];
         rr:predicateObjectMap [
                 rr:predicateMap [
                         a rr:PredicateMap;
                         rr:constant dcterms:modified;
                 ];
                 rr:objectMap [ rr:column "	year_modified"; rr:datatype xsd:integer];
         ];
         rr:predicateObjectMap [
                 rr:predicateMap [
                         a rr:PredicateMap;
                         rr:constant dcat:endpointDescription;
                 ];
                 rr:objectMap [ rr:column "	zipcodes"; rr:datatype xsd:string];
         ].
