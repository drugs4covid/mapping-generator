         rr:predicateObjectMap [
                 rr:predicate cmt:rejectPaper;
                 rr:objectMap [
                         rr:parentTriplesMap <#Paper>;
                         rr:joinCondition [
                                 rr:child "id";
                                 rr:parent "id";
